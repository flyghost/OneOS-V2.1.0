mainmenu "OneOS Configuration"

#Define the relative path to root directory of os code
OS_ROOT=../..
SRC_HAL=mm32

config SOC_MM32SPIN27PS
    bool
    select SERIES_MM32SPIN2XX
    default y
    
    if SOC_MM32SPIN27PS
        config SOC_MODEL
        string
        default "MM32SPIN27PS"
    endif
    
source "$OS_ROOT/Kconfig"
